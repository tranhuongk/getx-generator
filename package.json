{
  "name": "getx-generator",
  "displayName": "GetX Generator",
  "description": "GetX Generator will generate pattern and files",
	"publisher": "HuongTran",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.65.0"
  },
  "bugs": {
    "url": "https://github.com/tranhuongk/getx-generator/issues",
    "email": "huongtv.uet@gmail.com"
  },
  "icon": "assets/logo.webp",
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "keywords": [
    "dart",
    "flutter",
    "angulardart",
    "state-management",
		"getx"
  ],
  "activationEvents": [
    "onCommand:getx-generator.getxInstall",
    "onCommand:getx-generator.getxAddPage"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "getx-generator.getxInstall",
        "title": "GetX: (⚠️) Initialize App with getx_pattern "
      },
      {
        "command": "getx-generator.getxAddPage",
        "title": "GetX: Generate files (Add page)"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "replace-in-file": "^6.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tranhuongk/getx-generator.git"
  }
}